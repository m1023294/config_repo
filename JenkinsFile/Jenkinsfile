pipeline {
    agent any
    stages {
		stage('load') {
			steps {
				script {
					props = readProperties file: 'Properties/pipeline.properties'
					buildNo=BUILD_NUMBER
					echo 'load success'
				}
			}
		}
		stage('clone') {
			steps {
				script {
					sh props.CLEAN_WORKSPACE
					sh props.GIT_CLONE_UTIL_REPO
					ciCdUtility=load props.JAVA_CI_CD_GROOVY
					echo 'clone success'
					ciCdUtility.execute();
				}
			}
		}
	}
}